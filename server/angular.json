{
    "$schema": "./node_modules/@angular-devkit/core/src/workspace/workspace-schema.json",
    "version": 1,
    "newProjectRoot": "src",
    "projects": {
        "login": {
            "root": "src/main/typescript",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
            "aot": true,
                        "outputPath": "dist",
                        "main": "src/main/typescript/login/main.ts",
                        "tsConfig": "src/main/typescript/tsconfig.app.json",
                        "serviceWorker": false,
                        "index": "src/main/typescript/login/index.html",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "src/main/typescript/assets",
                                "output": "assets"
                            },
                            {
                                "glob": "**/*",
                                "input": "src/main/typescript/login/i18n",
                                "output": "login/i18n/"
                            },
                            {
                                "glob": "favicon.ico",
                                "input": "src/main/typescript",
                                "output": "favicon.ico"
                            }
                        ],
                        "styles": [
                            "src/main/typescript/styles.css",
                            "node_modules/bootstrap/dist/css/bootstrap.min.css"
                        ],
                        "scripts": [],
                        "polyfills": "src/main/typescript/polyfills.ts"
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "src": "src/main/typescript/environments/environment.ts",
                                    "replaceWith": "src/main/typescript/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/main/typescript/test.ts",
                        "polyfills": "src/main/typescript/polyfills.ts",
                        "tsConfig": "src/main/typescript/tsconfig.spec.json",
                        "karmaConfig": "src/test/typescript/karma.conf.js",
                        "styles": [],
                        "scripts": [],
                        "assets": []
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "login:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "login:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "login:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/main/typescript/tsconfig.app.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
