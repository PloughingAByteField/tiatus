{
  "name": "tiatus",
  "version": "0.0.1",
  "description": "Tiatus is a unified timing system",
  "private": true,
  "scripts": {
    "build:aot:prod": "npm run clean && npm run webpack -- --config webpack/webpack.prod.js --progress --profile --bail",
    "build:aot": "npm run build:aot:prod",
    "clean": "rimraf target/ts_compiled ${TARGET}/common ${TARGET}/vendor ${TARGET}/common ${TARGET}/polyfills ${TARGET}/adjudicator ${TARGET}/admin ${TARGET}/results ${TARGET}/timing ${TARGET}/setup ${TARGET}/assets",
    "build:login": "ng build login --output-path ${TARGET}/login --configuration production",
    "build:results": "ng build results --output-path ${TARGET}/results --configuration production",
    "build:admin": "ng build admin --output-path ${TARGET}/admin --configuration production",
    "build:adjudicator": "ng build adjudicator --output-path ${TARGET}/adjudicator --configuration production",
    "build:timing": "ng build timing --output-path ${TARGET}/timing --configuration production",
    "build:setup": "ng build setup --output-path ${TARGET}/setup --configuration production",
    "build": "npm run build:login && npm run build:results && npm run build:admin && npm run build:adjudicator && npm run build:timing && npm run build:setup",
    "ngc": "./node_modules/.bin/ngc -w -p tsconfig.webpack.json",
    "rimraf": "rimraf",
    "integration-test": "protractor webpack/protractor.conf.js",
    "postinstall": "npm run webdriver:update",
    "test": "ng test",
    "tsc": "tsc",
    "lint": "npm run tslint ",
    "tslint": "tslint --type-check --project tsconfig.webpack.json --exclude node_modules",
    "start": "webpack-dev-server --config webpack/webpack.dev.js --progress --port 8082",
    "specs-build": "webpack --config webpack/webpack.test.js",
    "webdriver:update": "webdriver-manager update",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
